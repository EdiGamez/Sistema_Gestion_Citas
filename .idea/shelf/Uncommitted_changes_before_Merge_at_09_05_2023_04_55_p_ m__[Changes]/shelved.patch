+import java.time.LocalDateTime;
+import java.time.format.DateTimeFormatter;
+import java.util.UUID;
+class Cita {
+    private UUID id;
+    private Paciente paciente;
+    private Medico medico;
+    private LocalDateTime fechaHora;
+    public enum EstatusCita {
+        PENDIENTE, REALIZADA
+    }
+    DateTimeFormatter formatter = DateTimeFormatter.ofPattern("yyyy-MM-dd HH:mm");
+
+    private EstatusCita estatus;
+
+    public Cita(UUID id, Paciente paciente, Medico medico, LocalDateTime fechaHora, EstatusCita estatus) {
+        this.id = id;
+        this.paciente = paciente;
+        this.medico = medico;
+        this.fechaHora = fechaHora;
+        this.estatus = estatus;
+    }
+    public Cita(Paciente paciente, Medico medico, LocalDateTime fechaHora) {
+        this(UUID.randomUUID(), paciente, medico, fechaHora, EstatusCita.PENDIENTE);
+    }
+    public UUID getId() {
+        return id;
+    }
+
+    public Paciente getPaciente() {
+        return paciente;
+    }
+    public Medico getMedico() {
+        return medico;
+    }
+    public String toCSV() {
+        return id + "," + paciente.getId() + "," + medico.getId() + "," + fechaHora.format(formatter) + "," + estatus;
+    }
+    public String toString() {
+        return
+
+                " Paciente: " + paciente.getNombre() + " "+paciente.getApellido()
+                        +"\n Medico: " + medico.getNombre() + " "+medico.getApellido()+
+                ",\n Fecha y Hora: " + fechaHora.format(formatter) +
+                ",\n Estatus: " + estatus+"\n ID: " + getId();
+    }
+
+}
